{
	"useTabs": true,
	"trailingComma": "none",
	"printWidth": 120,
	"plugins": ["prettier-plugin-astro", "prettier-plugin-vue", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.astro",
			"options": {
				"parser": "astro"
			}
		},
		{
			"files": "*.vue",
			"options": {
				"parser": "vue"
			}
		}
	]
}
